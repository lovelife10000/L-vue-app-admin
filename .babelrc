{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 5%",
            "ie > 9"
          ],
          "uglify": true
        }
      }
    ]
  ],
  "plugins": [
    "babel-polyfill",
    "transform-runtime",
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ],
  "comments": false
}